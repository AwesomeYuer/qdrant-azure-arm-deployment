{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "containergroupname": {
            "value": "qdrantonazure"
        },
        "containername": {
            "value": "azqdrantcontainer"
        },
        "location": {
            "value": "westus"
        },
        "imageName": {
            "value": "qdrant/qdrant"
        },
        "volumeName": {
            "value": "azqdrantvolume"
        },
        "numberCpuCores": {
            "value": "4"
        },
        "memory": {
            "value": "16"
        },
        "ipAddressType": {
            "value": "Public"
        },
        "storageaccountname": {
            "value": "qdrantstorage"
        },
        "storageaccountkey": {
            "value": "mykey"
        },
        "storagesharename": {
            "value": "aciqdrantshare"
        },
        "ports": {
            "value": [
                {
                    "port": "6333",
                    "protocol": "TCP"
                },
                {
                    "port": "80",
                    "protocol": "TCP"
                }
            ]
        },
        "dnsNameLabel": {
            "value": "qdrant-aci-dns"
        }
    }
}